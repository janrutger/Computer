# .HEADER
. $random_seed 1
# .FUNCTIONS
@rt_rnd
    ldm A $random_seed
    call @push_A
    ldi A 134775813
    call @push_A
    call @rt_mul
    ldi A 1
    call @push_A
    call @rt_add
    ldi A 65536
    call @push_A
    call @rt_mod
    call @pop_A
    sto A $random_seed
    ldm A $random_seed
    call @push_A
    ldi A 1000
    call @push_A
    call @rt_mod
    ret

# .DATA
% $random_seed 54321
