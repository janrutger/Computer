# Test Script for SIMPL Target

# Function creation and WHILE loop
DEF gcd {
    WHILE DUP 0 != DO
        SWAP OVER %
    DONE
    DROP
    
}

MACRO sum {
    test b gcd AS result
}

VALUE test 12
VALUE b 30
VALUE result 0

CONST jr 43


## Not supported yet, 
# VAR jr 10
# LIST jr 3
# STRING jr "JR"
# \"JRK"
# USE std_deque
# INCLUDE std_deque
# ASM {}


# Calling an function/Subroutine

sum result PRINT


test b > IF
    1 PRINT
ELSE
    2 PRINT
END

RND NEGATE DUP ABS PRINT PRINT

jr 1 - PRINT



IO 1 ONLINE
IO 1 NEW

10 IO 1 DRAW
42 IO 1 DRAW

IO 2 ONLINE
IO 2 NEW

