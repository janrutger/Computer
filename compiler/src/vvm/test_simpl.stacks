# Test Script for SIMPL Target

# Function creation and WHILE loop
DEF gcd {
    WHILE DUP 0 != DO
        SWAP OVER %
    DONE
    DROP
    
}

MACRO sum {
    test b gcd AS result
}

VALUE test 12
VALUE b 30
VALUE result 0

